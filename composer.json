{
    "name": "dlds/yii2-translate-manager",
    "description": "Online Translate",
    "type": "yii2-extension",
    "keywords": ["yii2", "extension", "translate", "language", "module"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dlds/yii2-translate-manager/issues?state=open",
	      "source": "https://github.com/dlds/yii2-translate-manager"
	},
    "authors": [
        {
            "name": "Lajos Molnar",
            "email": "lajax.m@gmail.com"
        },
        {
            "name": "Jiri Svoboda",
            "email": "jiri.svoboda@dlds.cz"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
        "yiisoft/yii2-jui": "*",
        "yiisoft/yii2-bootstrap": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "dlds\\translatemanager\\": ""
        }
    },
    "scripts": {
        "cs-fix": "php-cs-fixer fix --config=.php_cs",
        "cs-fix-dry-run": "php-cs-fixer fix --config=.php_cs --dry-run --diff"
    }
}
